{% set root = './' %}
{% set pageTitle = "АВТО В НАЯВНОСТІ" %}

{% extends "layout.tmpl.njk" %}
{% block content %}

  {# Header #}
  {% include "blocks/header.tmpl.njk" %}

  {# Banner #}
  {% set banner = {
    bgImg: 'assets/images/bg2.jpg',
    title: 'АВТО В НАЯВНОСТІ'
  } %}
  {% include "blocks/banner.tmpl.njk" %}

  {# Bread Crumbs #}
  {% set breadcrumbs = {
    items: [
      {
        link_href: "index.html",
        link_text: "Головна"
      },
      {
        link_href: "cars.html",
        link_text: "Авто в наявності"
      }
    ]
  } %}
  {% include "blocks/bread-crumbs.tmpl.njk" %}

  {# Products #}
  <section class="products">
    {% import "blocks/products.tmpl.njk" as product %}
    {%- call product.main() -%}
    {%- call product.row() -%}
    {%- call product.item(imgPath = root + 'assets/images/car1.jpg', title = 'Mercedes-Benz GLE-Class 2018', price = '51 962 €') -%}
    {%- endcall -%}
    {%- call product.item(imgPath = root + 'assets/images/car2.jpg', title = 'Audi Q7 3.0 TDI S-LINE 2018', price = '87 898 €') -%}
    {%- endcall -%}
    {%- call product.item(imgPath = root + 'assets/images/car3.jpg', title = 'Land Rover Range Rover Sport 2018', price = '78 132 €') -%}
    {%- endcall -%}
    {%- call product.item(imgPath = root + 'assets/images/car4.jpg', title = 'Bentley Bentayga Diesel 2018', price = '215 000 €') -%}
    {%- endcall -%}
